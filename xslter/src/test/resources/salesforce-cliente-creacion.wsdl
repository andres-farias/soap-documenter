<?xml version="1.0" encoding="UTF-8"?>
<WL5G3N0:definitions name="CrearClienteBice" targetNamespace="cl.bice.apps.wsdl2html.http://www.example.org/CrearClienteBice/"
                     xmlns:WL5G3N0="cl.bice.apps.wsdl2html.http://schemas.xmlsoap.org/wsdl/"
                     xmlns:WL5G3N1="cl.bice.apps.wsdl2html.http://www.example.org/CrearClienteBice/"
                     xmlns:WL5G3N2="cl.bice.apps.wsdl2html.http://schemas.xmlsoap.org/wsdl/soap/">

    <!-- Declaración de tipos -->
    <WL5G3N0:types>
        <WL5G3N0:documentation>Los tipos utilizados para realizar las operaciones.</WL5G3N0:documentation>
        <xsd:schema elementFormDefault="qualified"
                    targetNamespace="cl.bice.apps.wsdl2html.http://www.example.org/CrearClienteBice/"
                    xmlns:tns="cl.bice.apps.wsdl2html.http://www.example.org/CrearClienteBice/"
                    xmlns:xsd="cl.bice.apps.wsdl2html.http://www.w3.org/2001/XMLSchema">


            <xsd:element name="CrearCleinteBice">

                <xsd:annotation>
                    <xsd:documentation xml:lang="es">Estructura de datos para el payload de entrada que reunen todos los
                        datos necesarios para crear un cliente.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="indicadorCif" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="es">El indicador CIF es requerido como evidencia de la
                                    creación del usuario en CIF. Este valor es alfanumérico.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="indicadorSebra" type="xsd:string"/>
                        <xsd:element name="indicadorMagic" type="xsd:string"/>
                        <xsd:element name="Cif" type="tns:CifRequestType"/>
                        <xsd:element name="Sebra" type="tns:SebraRequestType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="CrearCleinteBiceResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="CifResponse" type="tns:CifResponseType"/>
                        <xsd:element name="SebraResponse" type="tns:SebraResponseType"/>
                        <xsd:element name="MagicResponse" type="tns:MagicResponse"/>
                        <xsd:element name="OwpResponse" type="tns:OwpResponse"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name="CifRequestType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="COD_CANAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="GLOSA_APLICACION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="COD_SUCURSAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="COD_OPERADOR" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="TIPO_IDEN_CLIENTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="IDEN_CLIENTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="FEC_CREACION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_ROL_EMPLEADO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_JURIDI" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CLIENTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_CLIENTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SECTOR_ECONOM" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_CLIENTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_COR_DOMLEGAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_ACTIVIDAD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SECTOR" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CATDEU" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_RELA_BICE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_GRUP_RELA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_ESTA_DEUD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CARPETA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CASI_INTE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_SINO_SBIF" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_DECL_JURADA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_VERIF_DOMLEGAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFLG_FIRMA_COBPREJUD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_FIRMA_COBPREJUD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_TAX_ID" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SII_EXEIMP" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFLG_SII_CONTPRIMERACAT" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_ALERTA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_ALERTA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFLAG_MANDATO_INVERSION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_GIROSII" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_FRAUDE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFLG_EMPLEADO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_PATERNO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_MATERNO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_NOMBRES" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_NICKNAME" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_NACION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_RESIDENCIA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_PROFESION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_NACIM" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SEXO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_NIV_EDUCAC" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_ESTCIVIL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SEPBIENES" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CARGAS" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_ANTIG_VIV" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_DEFUNCION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_CERT_ECIVI" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_RAZON_SOCI" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_RAZON_SOCIAL_LEGAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_FANTASIA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_GRUPO_ECON" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_CONSTITUCION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_CLIEMPRESA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_WEB" type="xsd:string"/>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="DIRECCION" type="tns:direccion"/>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="EMAIL" type="tns:email"/>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="CONTACTO" type="tns:contacto"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPOCLI" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_SUBTIPOCLI" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CLAS_SEG" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CLAS_EDAD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_CLASIF" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CERTIFICA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PFEC_CERTIFICA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIP_CLI_CL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_FATCA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_INTIN" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_IDGIN" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_CLARIE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_PAGA_IVA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_AGRUPACION" type="xsd:string"/>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="GIRO" type="tns:giro"/>
                    <xsd:element minOccurs="0" name="PCOD_OPERADOR_COMEX" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PRUT_CONYUGE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_CONYUGE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_EMPLEADOR" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_CARGO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PEMAIL_COMERCIAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_EJECUTIVO_INV" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_RELA_CORRE" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="direccion">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PCOD_CIUDAD" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_COMUNA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_PAIS" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_POSTAL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_REGION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPODIR" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_COMPLEMENTO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_DIRECCION" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_OFI_POST" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CASILLA" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_NUMERO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_SECUENCIA" type="xsd:string"/>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="TELEFONO" nillable="true"
                                 type="tns:telefono"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="telefono">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="CODIGO_FONO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_FONO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_FONO" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="giro">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PGLS_GIRO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_PRINCIPAL" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="email">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PGLS_DIRMAIL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_SECUENCIA" type="xsd:decimal"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="contacto">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PCOD_CARGO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PCOD_TIPO_CONTACTO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PGLS_EMAIL" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_MATERNO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_NOMBRES" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNOM_PATERNO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CELULAR" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_CORRELATIVO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_FAX" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="PNUM_FONO" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="CifResponseType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="return" type="tns:cifInsupdCrearClienteOut"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="cifInsupdCrearClienteOut">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="CODIGO" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="MENSAJE" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="n_CIF_ID" type="xsd:string"/>
                    <xsd:element minOccurs="0" name="n_ERROR" type="xsd:decimal"/>
                    <xsd:element minOccurs="0" name="v_ERROR" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SebraRequestType">
                <xsd:sequence>
                    <xsd:element name="wsCreacionClientes" type="tns:wsCreacionClientes"/>
                </xsd:sequence>
            </xsd:complexType>


            <!-- Definición del Pauyload de entrada para la creación -->
            <xsd:complexType name="wsCreacionClientes">
                <xsd:sequence>

                    <!-- TIPO DE DOCUMENTO -->
                    <xsd:element name="tipoDocumento" type="xsd:string" default="RUT" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Tipo de documento del cliente. Valores posibles: 'RUT': Rol
                                Único Tributario, 'PAS': Pasaporte. Valor por defecto: 'RUT'
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="rut" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Número de documento del cliente. Ej.: 12564875- 4
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="cuenta" type="xsd:string" default="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Número cuenta asociada al cliente.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="tipoDeCuenta" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Tipo cuenta del cliente.
                                Valores:
                                <![CDATA[
                                    <ul>
                                        <li>PROPC57BIS: 57bis</li>
                                        <li>PROPCACART: Administración Cartera</li>
                                        <li>PROPCAPV: APV</li>
                                        <li>PROPCINTER: Intermediación</li>
                                    </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="estado" type="xsd:string" default="07" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Estado del cliente.
                                <![CDATA[
                                <ul>
                                    <li>07: Vigente</li>
                                    <li>08: Bloqueado</li>
                                </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="nacionalidad" type="xsd:string" default="CL">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Nacionalidad del cliente. ISO del país.
                                <![CDATA[
                                <ul>
                                    <li>CL: Chile</li>
                                    <li>AR: Argentina</li>
                                </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="codigoAgente" type="xsd:string" default="0000" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Código de agente en CRM. Ejemplo: 0001</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <!-- CELULA_42: se agrega default -->
                    <xsd:element name="naturalidad" type="xsd:string" default="NAT">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Indica si es persona jurídica o persona natural. Valores:
                                <![CDATA[
                                <ul>
                                    <li>NAT: Natural</li>
                                    <li>JUR: Jurídica</li>
                                </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <!-- CELULA_42: se agrega default -->
                    <xsd:element minOccurs="0" name="tipCli" type="xsd:string" default="NAT">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Tipo de cliente.
                                <![CDATA[
                                <ul>
                                    <li>NAT: Natural</ul>
                                    <li>JUR: Jurídico</ul>
                                </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="nombreFantasia" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Nombre de fantasía del cliente, cuando es persona
                                jurídica.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="razonSocial" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Nombre cliente, cuando es persona jurídica. Obligatoriedad
                                Condicional?
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="giro" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Código del giro de la persona. Ver códigos de actividades
                                económicas
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="apellidoPaterno" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Apellido paterno cliente, cuando es persona natural.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="apellidoMaterno" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Apellido Materno cliente, cuando es persona natural.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="nombres" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Nombre del cliente, cuando es persona natural.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="fechaNacimiento" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Fecha de nacimiento del cliente, cuando es persona
                                natural.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="segmentoCliente" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Segmento del cliente.
                                Valores:
                                <![CDATA[
                                    <ul>
                                        <li>SEGCOA: Segmento A</li>
                                        <li>SEGCOB: Segmento B</li>
                                        <li>SEGCOC: Segmento C</li>
                                    </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="sexo" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Sexo del cliente, cuando es persona natural.
                                Valores:
                                <![CDATA[
                                    <ul>
                                        <li>F: Femenino</li>
                                        <li>M: Masculino</li>
                                    </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="estadoCivil" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Estado civil del cliente, cuando es persona natural.
                                Valores:
                                <![CDATA[
                                    <ul>
                                        <li>ECIVIACUER: Conviviente Civil</li>
                                        <li>ECIVISOLTE: Soltero</li>
                                        <li>ECIVIDIVOR: Divorciado</li>
                                        <li>ECIVICASAD: Casado</li>
                                    </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="regimenConyugal" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Régimen conyugal del cliente cuando es persona natural y
                                está casado o tiene conviviente civil.
                                Valores:
                                <![CDATA[
                                    <ul>
                                        <li>RCONYCOMUN: Comunidad</li>
                                        <li>RCONYPARTG: Part. Ganancias</li>
                                        <li>RCONYSEPBI: Sep. Bienes</li>
                                        <li>RCONYSINRE: Sin Régimen</li>
                                        <li>RCONYSOCCO: Soc. Conyugal</li>
                                    </ul>
                                ]]>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="profesion" type="xsd:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Código profesión o actividad del cliente, cuando es
                                persona natural. Ej.: 14: Agente Comercial
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="cargo" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Cargo del cliente, cuando es persona natural y es
                                trabajador dependiente.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element minOccurs="0" name="empleador" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Razón social del empleador del cliente, cuando es persona
                                natural y es trabajador dependiente.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <!-- Es la dirección obligatoria realmente o es opcional? -->
                    <xsd:element maxOccurs="unbounded" name="direccion" nillable="true"
                                 type="tns:direccionDto">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Direcciones asociadas al cliente.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="telefono" type="tns:telefonoDto" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Teléfonos asociados al cliente.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="email" nillable="true" type="tns:emailDto">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Emails asociados al cliente.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="cuentaBancaria" nillable="true"
                                 type="tns:cuentaBancariaDto">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Cuentas bancarias que pueda tener el cliente.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="personaRelasionada" nillable="true"
                                 type="tns:personaRelacionadaDto">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="es">Personas que están relacionadas con el cliente.
                            </xsd:documentation>
